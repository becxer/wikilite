<% for (i in mds) { %>	
	<div class='mds'>
		<dl>
			<dt><b><%=mdname[i]%></b></dt>
			<dd><%-mds[i]%></dd>
		</dl>
		<div class='link'>URL : 
			<script type="text/javascript">
				var currentPage = unescape(location.href);

				if((currentPage.indexOf("<%=mdname[i]%>")) == -1)
					document.write("<a href='"+currentPage+"/<%=mdname[i]%>'>"+currentPage+"/<%=mdname[i]%></a>");
				else
					document.write("<a href='"+currentPage+"'>"+currentPage+"</a>");

			</script>
		</div> 
	</div>

<% } %>


