//--Facebook login memo
/* Post FB Login */
router.post('/fb', function(req,res){
    snctrl.add(req.body.user_id);
    req.session.user_id = req.body.user_id;
    res.redirect('/');
});

//SESSTION CONTROLL MODULE
var snctrl = require(libpath+'/libfbsession'); 
